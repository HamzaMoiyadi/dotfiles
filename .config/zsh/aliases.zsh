# Aliases to be used in zsh shell


alias python=python3
alias vim=nvim
alias ls="eza --header --icons -la"
alias nvim-config="nvim ~/.config/nvim/"
alias cd="z"

alias npmd="npm run dev"

alias yd="yarn dev"
alias yt="yarn test"
alias ys="yarn start"

alias gc="git commit"
alias ga="git add"
alias gaa="git add ."
alias gp="git push"
alias gpo="git push -u origin"
alias gmd="git merge development"

alias bund="bun dev"
